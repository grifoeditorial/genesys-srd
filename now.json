{
    "version": 2,
    "name": "genesys-srd",
    "builds": [{
        "src": "package.json",
        "use": "@now/static-build",
        "config": {
            "distDir": "public"
        }
    }],
    "routes": [{
        "src": "^/(.*).html",
        "headers": {
            "cache-control": "public,max-age=60,s-maxage=259200,must-revalidate"
        },
        "dest": "$1.html"
    }]
}